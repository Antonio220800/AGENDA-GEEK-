{
  "expo": {
    "name": "Agenda Geek",
    "slug": "agenda-geek-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",

    "icon": "./assets/images/jocasp.png",

    "splash": {
      "image": "./assets/images/jocasp.png",
      "resizeMode": "contain",
      "backgroundColor": "#2F67E6"
    },

    "assetBundlePatterns": [
      "**/*"
    ],

    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/jocasp.png",
        "resizeMode": "contain",
        "backgroundColor": "#2F67E6"
      }
    },

    "android": {
      "package": "com.agendageek.app",
      "splash": {
        "image": "./assets/images/jocasp.png",
        "resizeMode": "contain",
        "backgroundColor": "#2F67E6"
      }
    },

    "web": {
      "favicon": "./assets/favicon.png"
    },

    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ]
  }
}
